; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "Client",
"language": "GDScript",
"path": "res://core/network/client.gd"
}, {
"base": "",
"class": "MultiplayerClient",
"language": "GDScript",
"path": "res://core/network/multiplayer_client.gd"
}, {
"base": "Node",
"class": "Server",
"language": "GDScript",
"path": "res://core/network/server.gd"
} ]
_global_script_class_icons={
"Client": "",
"MultiplayerClient": "",
"Server": ""
}

[application]

config/name="ApeOS"
run/main_scene="res://main.tscn"
boot_splash/fullsize=false
config/icon="res://icon.png"

[autoload]

logger="*res://core/logger.gd"

[display]

window/size/width=2560
window/size/height=1600
window/size/resizable=false
window/size/fullscreen=true

[gdnative]

singletons=[ "res://webrtc/webrtc.tres" ]

[gui]

theme/use_hidpi=true

[rendering]

environment/default_clear_color=Color( 0, 0, 0, 1 )
environment/default_environment="res://default_env.tres"
